          <add type="SanteDB.Core.Rest.Behavior.MessageLoggingEndpointBehavior, SanteDB.Core, Version=1.9.1.0"/>
          <add type="SanteDB.Core.Rest.Behavior.MessageCompressionEndpointBehavior, SanteDB.Core, Version=1.9.1.0"/>
