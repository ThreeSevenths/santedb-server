<SanteDBConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://santedb.org/configuration">
  <sections>
    <add type="SanteDB.Core.Configuration.ResourceMergeConfigurationSection, SanteDB.Core.Api"/>
  </sections>
  <!-- Master Data Management Configuration -->
  <section xsi:type="ResourceMergeConfigurationSection">
    <resources>
      <add type="Patient" autoMerge="false" preserveOriginal="false">
        <matching>
          <config autoLink="true">$identity</config>
          <config autoLink="false">org.santedb.matching.patient.default</config>
        </matching>
      </add>
    </resources>
  </section>
</SanteDBConfiguration>